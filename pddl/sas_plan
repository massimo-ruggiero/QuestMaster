(gather-information elara windhaven)
(calm-stormy-path elara windhaven forestofwhispers)
(travel elara windhaven forestofwhispers)
(complete-faction-trial elara elves elvenfragment forestofwhispers)
(travel elara forestofwhispers cursedmarsh)
(defeat-bandits-on-path elara cursedmarsh dwarvenstronghold)
(travel elara cursedmarsh dwarvenstronghold)
(complete-faction-trial elara dwarves dwarvenfragment dwarvenstronghold)
(travel elara dwarvenstronghold fortressofshadows)
(confront-malakar-and-restore-crystal elara malakar fortressofshadows crystalfragment dwarvenfragment elvenfragment)
; cost = 10 (unit cost)
